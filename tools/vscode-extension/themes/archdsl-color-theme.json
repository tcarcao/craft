{
  "name": "Domain DSL Light",
  "type": "light",
  "colors": {
    "editor.background": "#ffffff",
    "editor.foreground": "#000000",
    "editor.lineHighlightBackground": "#f5f5f5",
    "editor.selectionBackground": "#add6ff"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment.line.double-slash.domain-dsl",
        "comment.block.domain-dsl"
      ],
      "settings": {
        "foreground": "#008000",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords (use_case, when)",
      "scope": "keyword.control.flow.domain-dsl",
      "settings": {
        "foreground": "#800080",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Block Keywords (services, arch, exposure)",
      "scope": [
        "keyword.control.services.domain-dsl",
        "keyword.control.arch.domain-dsl",
        "keyword.control.exposure.domain-dsl"
      ],
      "settings": {
        "foreground": "#800080",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Use Case Name",
      "scope": "string.quoted.double.usecase.domain-dsl",
      "settings": {
        "foreground": "#0070C1",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Service Name",
      "scope": "entity.name.type.service.domain-dsl",
      "settings": {
        "foreground": "#0070C1",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Property Keywords (domains, data-stores)",
      "scope": [
        "keyword.other.property.domains.domain-dsl",
        "keyword.other.property.datastores.domain-dsl",
        "keyword.other.property.language.domain-dsl",
        "keyword.other.property.deployment.domain-dsl",
        "keyword.other.property.to.domain-dsl",
        "keyword.other.property.of.domain-dsl",
        "keyword.other.property.through.domain-dsl"
      ],
      "settings": {
        "foreground": "#d73a49",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Event Strings",
      "scope": "string.quoted.double.event.domain-dsl",
      "settings": {
        "foreground": "#001080",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Domain Names (all types)",
      "scope": [
        "entity.name.type.domain.domain-dsl",
        "entity.name.type.domain.source.domain-dsl",
        "entity.name.type.domain.target.domain-dsl",
        "entity.name.type.domain.list.domain-dsl"
      ],
      "settings": {
        "foreground": "#267f99",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Datastore Names",
      "scope": "entity.name.type.datastore.domain-dsl",
      "settings": {
        "foreground": "#6f42c1",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Language",
      "scope": "entity.name.type.language.domain-dsl",
      "settings": {
        "foreground": "#6f42c1",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Actor Names",
      "scope": "entity.name.class.actor.domain-dsl",
      "settings": {
        "foreground": "#b8860b",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Regular Verbs",
      "scope": "entity.name.function.verb.domain-dsl",
      "settings": {
        "foreground": "#795e26",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Special Verb: asks (request)",
      "scope": "keyword.other.asks.domain-dsl",
      "settings": {
        "foreground": "#e36209",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Special Verb: notifies (async)",
      "scope": "keyword.other.notifies.domain-dsl",
      "settings": {
        "foreground": "#22863a",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Special Keywords (listens, CRON)",
      "scope": [
        "keyword.other.listens.domain-dsl",
        "keyword.other.cron.domain-dsl"
      ],
      "settings": {
        "foreground": "#6f42c1",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Connectors (a, an, the, to, from, etc.)",
      "scope": "keyword.other.connector.domain-dsl",
      "settings": {
        "foreground": "#6a737d"
      }
    },
    {
      "name": "Phrases (action descriptions)",
      "scope": "string.unquoted.phrase.domain-dsl",
      "settings": {
        "foreground": "#a31515",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Remainder (after actor verb)",
      "scope": "string.unquoted.remainder.domain-dsl",
      "settings": {
        "foreground": "#a31515"
      }
    },
    {
      "name": "Regular Strings",
      "scope": "string.quoted.double.domain-dsl",
      "settings": {
        "foreground": "#a31515"
      }
    },
    {
      "name": "Punctuation - Braces",
      "scope": [
        "punctuation.section.braces.begin.domain-dsl",
        "punctuation.section.braces.end.domain-dsl"
      ],
      "settings": {
        "foreground": "#b8860b",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Punctuation - Colons",
      "scope": "punctuation.separator.colon.domain-dsl",
      "settings": {
        "foreground": "#6a737d"
      }
    },
    {
      "name": "Punctuation - Commas",
      "scope": "punctuation.separator.comma.domain-dsl",
      "settings": {
        "foreground": "#6a737d"
      }
    },
    {
      "name": "Architecture Components",
      "scope": [
        "entity.name.type.component.domain-dsl",
        "entity.name.type.arch.domain-dsl"
      ],
      "settings": {
        "foreground": "#0451a5",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Architecture Sections",
      "scope": [
        "keyword.other.section.presentation.domain-dsl",
        "keyword.other.section.gateway.domain-dsl"
      ],
      "settings": {
        "foreground": "#d73a49",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Component Modifier Keys",
      "scope": [
        "entity.name.attribute.modifier-key.domain-dsl",
        "entity.name.attribute.modifier-flag.domain-dsl"
      ],
      "settings": {
        "foreground": "#0451a5",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Component Modifier Values",
      "scope": "constant.other.modifier-value.domain-dsl",
      "settings": {
        "foreground": "#22863a",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Exposure Elements",
      "scope": [
        "entity.name.type.exposure.domain-dsl",
        "entity.name.type.target.domain-dsl",
        "entity.name.type.gateway.domain-dsl"
      ],
      "settings": {
        "foreground": "#e36209",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Deployment Configuration",
      "scope": [
        "keyword.other.deployment-type.domain-dsl",
        "entity.name.type.deployment-target.domain-dsl"
      ],
      "settings": {
        "foreground": "#b8860b",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Percentages",
      "scope": "constant.numeric.percentage.domain-dsl",
      "settings": {
        "foreground": "#09885a",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Operators and Flow",
      "scope": [
        "keyword.operator.arrow.domain-dsl",
        "keyword.operator.component-flow.domain-dsl"
      ],
      "settings": {
        "foreground": "#d73a49",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Brackets",
      "scope": [
        "punctuation.section.brackets.begin.domain-dsl",
        "punctuation.section.brackets.end.domain-dsl"
      ],
      "settings": {
        "foreground": "#6f42c1"
      }
    },
    {
      "name": "Parentheses",
      "scope": "punctuation.section.parentheses.domain-dsl",
      "settings": {
        "foreground": "#795e26"
      }
    },
    {
      "name": "CRON Description",
      "scope": "string.unquoted.cron-description.domain-dsl",
      "settings": {
        "foreground": "#a31515",
        "fontStyle": "italic"
      }
    }
  ]
}