{
  "name": "tree-sitter-craft",
  "version": "1.0.0",
  "description": "Tree-sitter grammar for Craft DSL",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tcarcao/tree-sitter-craft.git"
  },
  "license": "MIT",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "craft",
    "dsl"
  ],
  "main": "bindings/node/index.js",
  "types": "bindings/node/index.d.ts",
  "exports": {
    ".": {
      "import": "./bindings/node/index.js",
      "require": "./bindings/node/index.js",
      "types": "./bindings/node/index.d.ts"
    }
  },
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.5.0",
    "node-gyp-build": "^4.8.4"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.25.10"
  },
  "peerDependencies": {
    "tree-sitter": "^0.25.0"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "install": "node-gyp-build",
    "test": "tree-sitter test",
    "build-wasm": "tree-sitter build --wasm"
  }
}
